{
  "version": 2,
  "builds": [
    { "src": "packages/ui/next.config.js", "use": "@vercel/next" },
    { "src": "packages/api/dist/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "packages/api/dist/index.js" },
    { "src": "/(.*)", "dest": "packages/ui/$1" }
  ]
}
